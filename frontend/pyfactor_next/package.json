{
  "name": "pyfactor_next",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@8.10.0",
  "scripts": {
    "dev": "USE_DATABASE=true MOCK_DATA_DISABLED=true PROD_MODE=true NEXT_PUBLIC_API_URL=https://127.0.0.1:8000 BACKEND_API_URL=https://127.0.0.1:8000 NODE_OPTIONS='--max-old-space-size=4096' next dev",
    "dev-fast": "NODE_OPTIONS='--max-old-space-size=2048' NEXT_TELEMETRY_DISABLED=1 USE_DATABASE=true MOCK_DATA_DISABLED=true PROD_MODE=true NEXT_PUBLIC_API_URL=https://127.0.0.1:8000 BACKEND_API_URL=https://127.0.0.1:8000 next dev --turbo",
    "dev-minimal": "NODE_OPTIONS='--max-old-space-size=1024' NEXT_TELEMETRY_DISABLED=1 next dev --turbo",
    "dev:oauth": "./scripts/dev-with-oauth.sh",
    "dev:https": "PORT=3000 NODE_TLS_REJECT_UNAUTHORIZED=0 HTTPS=true SSL_CRT_FILE=../../certificates/localhost+1.pem SSL_KEY_FILE=../../certificates/localhost+1-key.pem USE_DATABASE=true MOCK_DATA_DISABLED=true PROD_MODE=true NEXT_PUBLIC_API_URL=https://127.0.0.1:8000 BACKEND_API_URL=https://127.0.0.1:8000 NODE_OPTIONS='--max-old-space-size=4096' next dev --experimental-https -p 3000",
    "dev:db": "USE_DATABASE=true MOCK_DATA_DISABLED=true NODE_OPTIONS='--max-old-space-size=4096' next dev",
    "dev:prod": "USE_DATABASE=true MOCK_DATA_DISABLED=true PROD_MODE=true NODE_OPTIONS='--max-old-space-size=4096' next dev",
    "dev:real-api": "USE_DATABASE=true MOCK_DATA_DISABLED=true PROD_MODE=true NODE_OPTIONS='--max-old-space-size=4096' next dev",
    "dev:original": "NODE_OPTIONS='--max-old-space-size=4096' next dev",
    "dev:simple": "node simple-server.js",
    "dev:pnpm": "NEXT_CONFIG=simple ./pnpm-fix.sh",
    "dev-high-memory": "NODE_OPTIONS='--max-old-space-size=12288' DEBUG=pyfactor:* node server/server.js",
    "dev-debug": "NODE_OPTIONS='--max-old-space-size=12288 --inspect' DEBUG=pyfactor:* node server/server.js",
    "dev:server-https": "NODE_OPTIONS='--max-old-space-size=8192' node server/https-server.js",
    "monitor": "node scripts/monitor-memory.js",
    "memory-analysis": "node src/scripts/find-memory-leaks.js",
    "optimize-component": "node src/scripts/optimize-component.js",
    "test:console": "node scripts/test_console_logs.mjs",
    "build": "next build",
    "build:production": "NODE_ENV=production NEXT_PUBLIC_API_URL=https://api.dottapps.com BACKEND_API_URL=https://api.dottapps.com USE_DATABASE=true MOCK_DATA_DISABLED=true PROD_MODE=true NODE_OPTIONS=\"--max-old-space-size=3072\" next build",
    "build:production-clean": "echo 'ðŸš¨ FORCING PNPM BUILD - Enhanced OAuth v2.2 - CACHE CLEAR' && rm -rf .next && rm -rf node_modules/.cache && node scripts/check-env.js && node scripts/debug-stripe-env.js && NODE_ENV=production NEXT_PUBLIC_API_URL=https://api.dottapps.com BACKEND_API_URL=https://api.dottapps.com USE_DATABASE=true MOCK_DATA_DISABLED=true PROD_MODE=true NODE_OPTIONS=\"--max-old-space-size=8192\" next build",
    "build:production-fast": "NODE_ENV=production NEXT_PUBLIC_API_URL=https://api.dottapps.com BACKEND_API_URL=https://api.dottapps.com USE_DATABASE=true MOCK_DATA_DISABLED=true PROD_MODE=true NODE_OPTIONS=\"--max-old-space-size=8192\" next build --no-lint",
    "build:render": "NODE_ENV=production NODE_OPTIONS=\"--max-old-space-size=3072\" next build",
    "build:render-fast": "node scripts/fast-build.js",
    "build:render-clean": "rm -rf .next && NODE_ENV=production NODE_OPTIONS=\"--max-old-space-size=4096\" next build -c next.config.render.js",
    "build:turbo": "NODE_ENV=production NODE_OPTIONS=\"--max-old-space-size=3072\" next build --experimental-turbo",
    "build:ignore-errors": "DISABLE_ESLINT_PLUGIN=true NODE_OPTIONS=\"--max-old-space-size=8192\" next build",
    "export": "next export",
    "start": "next start",
    "start:render": "NODE_ENV=production next start -p $PORT",
    "start:https": "HTTPS=true SSL_CRT_FILE=../../certificates/localhost+1.pem SSL_KEY_FILE=../../certificates/localhost+1-key.pem NODE_OPTIONS=\"--max-old-space-size=8192\" next start",
    "start:debug": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "dev:optimized": "NODE_OPTIONS=\"--max-old-space-size=8192\" next dev",
    "build:optimized": "NODE_OPTIONS=\"--max-old-space-size=8192\" next build",
    "analyze": "ANALYZE=true next build",
    "start:optimized": "NODE_OPTIONS=\"--max-old-space-size=8192\" next start",
    "clear": "rm -rf .next && rm -rf node_modules/.cache",
    "fix": "./fix-modules.sh",
    "verify-tailwind": "node verify-tailwind-setup.js",
    "fix-auth0-logout": "node fix-auth0-logout.js",
    "load-test": "node scripts/load-test-sessions.js",
    "load-test:smoke": "node scripts/load-test-sessions.js --users=5 --duration=30 --scenarios=auth",
    "load-test:load": "node scripts/load-test-sessions.js --users=50 --duration=300 --scenarios=mixed",
    "load-test:stress": "node scripts/load-test-sessions.js --users=200 --duration=600 --scenarios=stress",
    "load-test:spike": "node scripts/load-test-sessions.js --users=500 --duration=180 --scenarios=auth",
    "load-test:endurance": "node scripts/load-test-sessions.js --users=100 --duration=3600 --scenarios=mixed",
    "load-test:cache": "node scripts/load-test-sessions.js --users=150 --duration=300 --scenarios=cache",
    "session-metrics": "curl -s http://localhost:3000/api/metrics/session?detailed=true | jq",
    "session-dashboard": "open http://localhost:3000/api/admin/session-dashboard",
    "session-health": "curl -s http://localhost:3000/api/metrics/session | jq '.status,.score,.cache_hit_rate,.avg_response_time'",
    "cache-status": "curl -s http://localhost:3000/api/cache/session | jq"
  },
