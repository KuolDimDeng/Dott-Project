{
  "name": "pyfactor_next",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@8.10.0",
  "scripts": {
    "dev": "NODE_OPTIONS='--max-old-space-size=4096' next dev",
    "dev:original": "node server/server.js",
    "dev:simple": "node simple-server.js",
    "dev:pnpm": "NEXT_CONFIG=simple ./pnpm-fix.sh",
    "dev-high-memory": "NODE_OPTIONS='--max-old-space-size=12288' DEBUG=pyfactor:* node server/server.js",
    "dev-debug": "NODE_OPTIONS='--max-old-space-size=12288 --inspect' DEBUG=pyfactor:* node server/server.js",
    "monitor": "node scripts/monitor-memory.js",
    "memory-analysis": "node src/scripts/find-memory-leaks.js",
    "optimize-component": "node src/scripts/optimize-component.js",
    "build": "next build",
    "build:ignore-errors": "DISABLE_ESLINT_PLUGIN=true NODE_OPTIONS=\"--max-old-space-size=8192\" next build",
    "start": "next start",
    "start:debug": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "dev:optimized": "NODE_OPTIONS=\"--max-old-space-size=8192\" next dev",
    "build:optimized": "NODE_OPTIONS=\"--max-old-space-size=8192\" next build",
    "analyze": "ANALYZE=true next build",
    "start:optimized": "NODE_OPTIONS=\"--max-old-space-size=8192\" next start",
    "clear": "rm -rf .next && rm -rf node_modules/.cache",
    "fix": "./fix-modules.sh"
  },
  "dependencies": {
    "@aws-amplify/adapter-nextjs": "^1.5.0",
    "@aws-amplify/auth": "^6.10.0",
    "@aws-amplify/core": "^6.10.0",
    "@aws-sdk/client-cognito-identity-provider": "^3.782.0",
    "@headlessui/react": "^2.2.0",
    "@heroicons/react": "^2.2.0",
    "@prisma/client": "^6.5.0",
    "@react-pdf/renderer": "^4.3.0",
    "aws-amplify": "^6.10.0",
    "axios": "^1.8.4",
    "clsx": "^2.1.0",
    "countries-list": "^3.1.1",
    "date-fns": "2.30.0",
    "debug": "^4.3.4",
    "file-saver": "^2.0.5",
    "i18next": "^24.2.3",
    "i18next-browser-languagedetector": "^8.0.4",
    "js-cookie": "^3.0.5",
    "jsbarcode": "^3.11.6",
    "jspdf": "^3.0.1",
    "jspdf-autotable": "^5.0.2",
    "jwt-decode": "^4.0.0",
    "lodash": "^4.17.21",
    "next": "15.2.2",
    "next-auth": "^4.24.11",
    "pg": "^8.14.1",
    "prisma": "^6.5.0",
    "qrcode.react": "^4.2.0",
    "react": "^18.2.0",
    "react-cookie": "^8.0.1",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^5.0.0",
    "react-hook-form": "^7.55.0",
    "react-hot-toast": "^2.5.2",
    "react-i18next": "^15.4.1",
    "react-plaid-link": "^3.6.1",
    "react-select-country-list": "^2.2.3",
    "react-table": "^7.8.0",
    "react-toastify": "^11.0.5",
    "recharts": "^2.15.2",
    "xlsx": "^0.18.5",
    "zod": "^3.24.2",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@svgr/webpack": "^8.0.1",
    "@tailwindcss/forms": "^0.5.10",
    "@types/node": "22.14.0",
    "@types/react": "19.1.0",
    "@types/react-dom": "19.1.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.1.1",
    "mini-css-extract-plugin": "^2.9.2",
    "null-loader": "^4.0.1",
    "postcss": "^8.5.3",
    "postcss-import": "^16.1.0",
    "postcss-loader": "^8.1.1",
    "prettier": "^3.2.4",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "tailwindcss": "^3.4.17",
    "typescript": "5.8.3"
  },
  "resolutions": {
    "@aws-amplify/auth": "^6.10.0",
    "@aws-amplify/core": "^6.10.0",
    "@aws-amplify/adapter-nextjs": "^1.5.0",
    "aws-amplify": "^6.10.0"
  },
  "pnpm": {
    "overrides": {
      "@aws-amplify/auth": "^6.10.0",
      "@aws-amplify/core": "^6.10.0",
      "@aws-amplify/adapter-nextjs": "^1.5.0",
      "aws-amplify": "^6.10.0",
      "debug": "^4.3.7"
    },
    "peerDependencyRules": {
      "allowAny": [
        "debug"
      ]
    }
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not op_mini all",
    "not dead",
    "chrome >= 60",
    "firefox >= 60",
    "safari >= 12",
    "edge >= 18",
    "ios >= 12"
  ]
}
